        <activity
            android:name="com.example.activities.EnterPrescriptionInfoActivity"
            android:label="@string/title_activity_enter_prescription_info" >
        </activity>
        <activity
            android:name="com.example.activities.UrgencyListViewActivity"
            android:label="@string/title_activity_urgency_list_view" >
        </activity>
        <activity
            android:name="com.example.activities.ForceAccessDisplayActivity"
            android:label="@string/title_activity_force_access_display" >
        </activity>
        <activity
            android:name="com.example.activities.FillPrescriptionActivity"
            android:label="@string/title_activity_fill_prescription" >
