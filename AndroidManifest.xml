<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.triageapplication"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="Hospital Triage"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.activities.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.triageapplication.HomePageActivity"
            android:label="@string/title_activity_home_page" >
            <intent-filter>
                <action android:name="android.intent.action.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.triageapplication.RegisterNurseActivity"
            android:label="@string/title_activity_register_nurse" >
        </activity>
        <activity
            android:name="com.example.activities.ReLoginActivity"
            android:label="@string/title_activity_re_login" >
        </activity>
        <activity
            android:name="com.example.activities.CreatePatientActivity"
            android:label="@string/title_activity_create_patient" >
        </activity>
        <activity
            android:name="com.example.activities.UpdatePatientActivity"
            android:label="@string/title_activity_update_patient" >
        </activity>
        <activity
            android:name="com.example.activities.AccessRecordActivity"
            android:label="@string/title_activity_access_record" >
        </activity>
         <activity
            android:name="com.example.activities.PharmacistHomePageActivity"
            android:label="@string/title_pharmacist_homepage" >
        </activity>
        <activity
            android:name="com.example.activities.EnterUpdateInfoActivity"
            android:label="@string/title_activity_enter_update_info" >
        </activity>
        <activity
            android:name="com.example.activities.RetryAddPatientActivity"
            android:label="@string/title_activity_retry_add_patient" >
        </activity>
        <activity
            android:name="com.example.activities.ForceAccessRecordActivity"
            android:label="@string/title_activity_force_access_record" >
        </activity>
        <activity
            android:name="com.example.activities.RetryUpdateInfo"
            android:label="@string/title_activity_retry_update_info" >
